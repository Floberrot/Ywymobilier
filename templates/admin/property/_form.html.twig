{{ form_start(form) }}
<div class="row">
    <div class="md-8">
        <div class="row">
    <div class="col-md-4">{{ form_row(form.title) }}</div>
    <div class="col-md-4">{{ form_row(form.surface) }}</div>
    <div class="col-md-4">{{ form_row(form.price) }}</div>
        </div>
    </div>
    <div class="col md-4">
        <div class="row flex">

        <div class="col-md-4">
            <img src="{{ vich_uploader_asset(picture, 'imageFile') }}">

{#<img src="/images/properties/5e6771cac2ae2487829612.jpg">#}
        </div>

        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-2">{{ form_row(form.rooms) }}</div>
    <div class="col-md-2">{{ form_row(form.floor) }}</div>
</div>
<div class="row">
    <div class="col-md-5">{{ form_row(form.pictureFiles) }}</div>
    <div class="col-md-2">{{ form_row(form.city) }}</div>
    <div class="col-md-5">{{ form_row(form.address) }}</div>
    <div class="col-md-2">{{ form_row(form.cp) }}</div>
    <div class="col-md-1">{{ form_row(form.locate) }}</div>
</div>
{{ form_widget(form) }}
<button class="btn btn-success">{{ button|default('Ajouter ce bien') }}</button>
{{ form_end(form) }}